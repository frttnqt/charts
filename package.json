{
  "name": "charts-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test-cover": "ng test  --code-coverage --source-map=false",
    "test-cover:local": "ng test  --code-coverage --source-map=true",
		"test": "npm run test-cover:local",
    "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\" --write",
    "prettier:check:project": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\" --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,css,html}": [
      "eslint . --fix",
      "npm run prettier:check"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
	"private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^17.1.1",
    "@angular-eslint/eslint-plugin-template": "^17.1.1",
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@swimlane/ngx-charts": "^20.5.0",
    "date-fns": "^3.0.0-alpha.2",
    "ng-antd-color-picker": "^1.1.0",
    "ng-zorro-antd": "^17.0.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular-eslint/template-parser": "^17.1.1",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "autoprefixer": "^10.4.16",
    "eslint": "8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^4.3.8",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
		"lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.2"
  }
}
